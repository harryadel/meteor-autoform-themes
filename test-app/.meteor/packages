# Meteor packages used by this project, one per line.
meteor-base@1.5.1             # Packages every Meteor app needs to have
mobile-experience@1.1.0       # Packages for a great mobile UX
mongo                  # The database Meteor supports right now
blaze-html-templates@2.0.0    # Compile .html files into Meteor Blaze views
jquery@3.0.0                  # Wrapper package for npm-installed jquery
reactive-var@1.0.12          # Reactive variable for tracker
tracker@1.3.2                # Meteor's client-side reactive programming library
standard-minifier-css   # CSS minifier run for production mode
standard-minifier-js    # JS minifier run for production mode
es5-shim@4.8.0               # ECMAScript 5 compatibility for older browsers
ecmascript@0.16.7            # Enable ECMAScript2015+ syntax in app code
typescript             # Enable TypeScript syntax in .ts and .tsx files
shell-server@0.5.0           # Server-side component of the `meteor shell` command
hot-module-replacement@0.5.3  # Update client in development without reloading
blaze-hot               # Update files using Blaze's API in development without reloading

# AutoForm and its themes
aldeed:autoform@8.0.0-rc.4
aldeed:collection2
communitypackages:autoform-bootstrap3
communitypackages:autoform-bootstrap4
communitypackages:autoform-bootstrap5
communitypackages:autoform-plain
communitypackages:autoform-tailwind
